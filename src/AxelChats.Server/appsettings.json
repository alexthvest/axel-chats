{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Auth": {
    "Issuer": "https://localhost:5000",
    "Audience": "https://localhost:5000",
    "SecretKey": "SECRET_KEY",
    "AccessTokenLifeTime": 10
  },
  "ConnectionStrings": {
    "DefaultConnection": "DB_CONNECTION_STRING"
  }
}
